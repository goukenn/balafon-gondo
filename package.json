{
  "name": "balafon-bview-file-extesion",
  "displayName": "BALAFON bview file extesion",
  "description": "handle bview file on balafon framework",
  "publisher": "igkdev",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "icon":"./assets/img/main_logo@3x.png",
  "keywords":[
    "balafon",
    "bview"
  ],
  "categories": [
    "Programming Languages",
    "Other",
    "Snippets",
    "Testing" 
  ],
  "contributes": {
    "languages": [{
      "id": "bview",
      "aliases": ["BALAFON bview", "bview"],
      "extensions": [".bview"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bview",
      "scopeName": "source.demo.bview",
      "path": "./syntaxes/bview.demo.tmLanguage.json"
    }],
    "themes": [ 
      {
        "path": "./themes/bview-demo.color-theme.json",
        "uiTheme": "vs-dark",
        "label": "Demo bview theme dark",
        "id":"balafon-bview-theme"
      }
    ]
  }
}
